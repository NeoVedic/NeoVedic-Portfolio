modules = ["nodejs-20", "web", "php-8.2"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php -S 0.0.0.0:5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34059
externalPort = 3000

[[ports]]
localPort = 35069
externalPort = 4200

[[ports]]
localPort = 36441
externalPort = 3001

[[ports]]
localPort = 36639
externalPort = 3002

[[ports]]
localPort = 42857
externalPort = 5000

[[ports]]
localPort = 46339
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["php", "-S", "0.0.0.0:5000"]
